<!-- 地图 -->
<map
  id="map"
  longitude="{{long}}"
  latitude="{{lat}}"
  scale="15"
  show-location
  enable-zoom="{{true}}"
  enable-scroll="{{true}}"
  style="width: 100%; height: 200px;"
></map>


<van-button type="danger"  custom-class="check-in">

    签到
    <view class="time-now">{{hour}}:{{mint}}:{{second}}</view>

</van-button>


<van-steps
  steps="{{ steps }}"
  active="{{ active }}"
  direction="vertical"
  active-color="#f44"
/>