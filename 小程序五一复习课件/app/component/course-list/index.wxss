.course-list{
  width: 100%
}

.course-list::after{
  content: "";
  display: block;
  clear: both
}

.course-list dl{
  display: block;
  float: left;
  width: 48%;
  margin:0 1%; 
}

.course-list dl dt{
  width: 100%;
  display: block;
}

.course-list dl dt image{
  width: 100%;
  height: 190rpx
}

.course-list dl dd{
  display: block;
  width: 100%;
  height: 60rpx;
  font-size: 28rpx;
  line-height: 60rpx
}

.coursenav{
  width: 100%;
  height: 50rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #333333;
  display: flex;
  box-sizing: border-box;
  padding: 0 10rpx
  
}

.left, .right{
  flex: 1
}

.left{
  text-align: left
}

.right{
  text-align: right
}