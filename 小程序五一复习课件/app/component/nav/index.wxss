.nav{
  width: 100%;
  padding: 36rpx 0 14rpx
}

.nav::after{
  content: "";
  display: block;
  clear: both
}

.nav dl{
  float: left;
  width: 20%;
}

.nav dl dt{
  width: 100%;
  height: 90rpx;
  text-align: center;
  display: block
  
}

.nav dl dt image{
  width: 90rpx;
  height: 90rpx
}

.nav dl dd{
  display: block;
  text-align: center;
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  color: #404040
